{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "prettier"
    ],
    "overrides": [
        {
            "files": ["src/**"],
            "rules": {
                "react/react-in-jsx-scope": "off",
                "@typescript-eslint/no-confusing-void-expression": "off"
            }
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {}
}
